set ignorecase
set ts=4
set wso=vertical
set eld=shortarrow
set hls
map <F9> :until<cr>
#highlight Arrow cterm=bold,underline ctermfg=Red ctermbg=white
highlight SelectedLineNr ctermfg=LightRed
highlight Arrow ctermfg=LightRed

# SPC : toggle breakpoint
# a : list args
map a iinfo<Space>args<CR><ESC>
# bt : backtrace
# map bt ibt<CR><ESC>
map b ib
map c :continue<CR>
map d :down<CR>
map e :set<Space>dis<CR>
map E :set<Space>nodis<CR>
# f : finish
map f :finish<CR>
# n : next
map n :next<CR>
# o : open file dialog
# r : run
map r :run<CR>
map s :step<CR>
# t : temp Breakpoint
map t it<Space>
map u :up<CR>
map U :until<CR>
map v iinfo<Space>locals<CR><ESC>
